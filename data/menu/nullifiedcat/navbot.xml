<Tab name="Navbot" padding="6 6 6 6">
    <Box padding="12 6 6 6" width="content" height="content" name="NavBot (WalkBot)" >
        <List width="150">
            <AutoVariable width="fill" target="nav.enabled" label="Enable pathing"/>
            <AutoVariable width="fill" target="nav.draw" label="Draw path"/>
            <AutoVariable width="fill" target="nav.look-at-path" label="Look at path"/>
            <AutoVariable width="fill" target="nav.look-at-legit" label="Look at legit"/>
            <AutoVariable width="fill" target="navbot.enabled" label="Enable NavBot"/>
            <AutoVariable width="fill" target="navbot.capture-objectives" label="Capture objectives" tooltip="Automatically capture objectives (CTF, CP, and PL only)."/>
	    <AutoVariable width="fill" target="nav.look-crumbs" label="Look crumbs" min="0" max="10"/>
            <AutoVariable width="fill" target="navbot.search-health" label="Search health"/>
            <AutoVariable width="fill" target="navbot.search-ammo" label="Search ammo"/>
            <AutoVariable width="fill" target="navbot.escape-danger" label="Escape danger"/>
            <AutoVariable width="fill" target="navbot.escape-danger.slight-danger.capping" label="Safe capping" tooltip="Make bots run pre-emptively from capture points in case of danger."/>
            <AutoVariable width="fill" target="navbot.escape-danger.ctf-cap" label="Escape danger w. intel" tooltip="Also try to escape from danger when carrying intel."/>
            <AutoVariable width="fill" target="navbot.primary-only" label="Auto best weapon" tooltip="Auto Change weapon when Player near in your area"/>
	    <AutoVariable width="fill" target="navbot.draw-danger" label="Draw danger area"/>
	    <AutoVariable width="fill" target="navbot.engineer-mode" label="Engineer mode" tooltip="Making Navbot to Build Sentry and Dispenser in intel base with engineer class Recommended to using in CTF mode"/>
            <LabeledObject width="fill" label="Weapon slot">
                <Select target="navbot.force-slot">
                    <Option name="Don't force" value="0"/>
                    <Option name="Primary" value="1"/>
                    <Option name="Secondary" value="2"/>
                    <Option name="Melee" value="3"/>
                    <Option name="PDA" value="4"/>
                    <Option name="PDA2" value="5"/>
                </Select>
            </LabeledObject>
            <AutoVariable width="fill" target="navbot.autojump.enabled" label="Enable autojump"/>
            <AutoVariable width="fill" target="navbot.autojump.trigger-distance" label="Jump distance"/>
            <AutoVariable width="fill" target="navbot.snipe-sentries" label="Try to target sentries"/>
            <AutoVariable width="fill" target="navbot.snipe-sentries.shortrange" label="Scout/Pyro sentry mode" tooltip="Try to target sentries as scout/pyro aswell. recommended to pick class Scout , Pyro and Demoman."/>
            <AutoVariable width="fill" target="navbot.stay-near" label="Stalk enemies" tooltip="Try to outrange as sniper and close distance as scout/heavy"/>
            <!-- <!AutoVariable width="fill" target="navbot.take-teleporters" label="take teleporters"/> //coming soon...-->
        </List>
    </Box>
    <Box padding="12 6 6 6" width="content" height="content" name="Shield aimbot" x="165">
        <List width="150">
            <AutoVariable width="fill" target="chargeaim.enable" label="Enabled" tooltip="Makes your demoman charge automatically go towards the enemy. Only active when key is held, and does not trigger the charge itself."/>
            <AutoVariable width="fill" target="chargeaim.key" label="Key to hold"/>
        </List>
    </Box>
    <Box padding="12 6 6 6" width="content" height="content" name="Jumping" x="165" y="45">
        <List width="150">
            <AutoVariable width="fill" target="bunnyhop.enable" label="Enable bunny hop"/>
            <AutoVariable width="fill" target="bunnyhop.chance" label="Bhop chance" tooltip="% chance of a successful bunnyhop."/>
            <LabeledObject width="fill" label="Auto strafe">
                <Select target="misc.autostrafe">
                    <Option name="Off" value="0"/>
                    <Option name="Regular" value="1"/>
                    <Option name="Full Control" value="2"/>
                </Select>
            </LabeledObject>
        </List>
    </Box>
    <Box padding="12 6 6 6" width="content" height="content" name="Misc" x="330">
        <List width="150">
            <AutoVariable width="fill" target="misc.no-push" label="Nopush"/>
            <AutoVariable width="fill" target="misc.roll-speedhack" label="Moonwalk" tooltip="Increase crouched movement speed."/>
            <AutoVariable width="fill" target="misc.forward-speedhack-pitch-down" label="Forward Pitch down" tooltip="Roll Duck speed forward pitch down"/>
            <AutoVariable width="fill" target="misc.forward-speedhack-pitch-up" label="Forward Pitch up" tooltip="Roll Duck speed forward pitch up"/>
        </List>
    </Box>
</Tab>
